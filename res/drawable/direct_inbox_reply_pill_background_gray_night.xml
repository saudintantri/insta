<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/grey_5" />
            <solid android:color="@color/grey_9" />
            <corners android:radius="@dimen/background_mode_button_corner_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/bar_color_0_percent" />
            <solid android:color="@color/fds_transparent" />
            <corners android:radius="@dimen/background_mode_button_corner_radius" />
        </shape>
    </item>
</selector>