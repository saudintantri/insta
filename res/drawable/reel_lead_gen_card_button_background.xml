<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/igds_graph_low" />
            <corners android:radius="@dimen/abc_button_padding_horizontal_material" />
            <stroke android:width="1.0dip" android:color="@color/igds_graph_low" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/default_cta_dominate_color" />
            <corners android:radius="@dimen/abc_button_padding_horizontal_material" />
            <stroke android:width="1.0dip" android:color="@color/default_cta_dominate_color" />
        </shape>
    </item>
</selector>